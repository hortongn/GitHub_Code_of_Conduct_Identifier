<p id="notice"><%= notice %></p>

<p>
  <%= link_to 'Create New Repository Set', new_repository_loader_path %>
</p>

<table>
  <thead>
    <tr>
      <th>Existing Sets</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @repository_sets.each do |repository_set| %>
      <tr>
        <td><%= link_to repository_set.name, repository_set %></td>
        <td><%= link_to 'Destroy', repository_set, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

